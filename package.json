{
  "name": "js-webdriver-cucumber",
  "version": "1.0.0",
  "description": "Test automation with WebriverIO and Cucumber",
  "main": "index.js",
  "scripts": {
    "test:bdd": "wdio run ./wdio.conf.js",
    "test:bdd:smoke": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression '@smoke'",
    "test:bdd:negative": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression '@negative'",
    "test:bdd:wip": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression '@wip'",
    "generate:allure": "allure generate reports/allure-results/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SmotritelTerve/js-webdriver-cucumber.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SmotritelTerve/js-webdriver-cucumber/issues"
  },
  "homepage": "https://github.com/SmotritelTerve/js-webdriver-cucumber#readme",
  "devDependencies": {
    "@wdio/allure-reporter": "^7.10.1",
    "@wdio/cli": "^7.12.2",
    "@wdio/cucumber-framework": "^7.12.2",
    "@wdio/local-runner": "^7.12.2",
    "@wdio/spec-reporter": "^7.10.1",
    "chromedriver": "^107",
    "fs-extra": "^10.0.0",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "wdio-chromedriver-service": "^7.2.0",
    "wdio-cucumberjs-json-reporter": "^4.1.3"
  },
  "dependencies": {
    "moment": "^2.29.1"
  }
}
